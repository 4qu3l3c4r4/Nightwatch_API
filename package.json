{
  "name": "nightwatch-api",
  "version": "0.3.0",
  "main": "src/index.js",
  "engines": {
    "node": ">= 8.0.0"
  },
  "repository": "git@github.com:mucsi96/nightwatch-api.git",
  "author": "Igor Mucsicska <mucsi_96@yahoo.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsc --emitDeclarationOnly && babel src --out-dir lib --extensions \".ts,.tsx\"",
    "type-check": "tsc --noEmit",
    "start": "cross-env DEBUG=nightwatch-api node test/server.js",
    "test": "cross-env DEBUG=nightwatch-api jest",
    "debug":
      "cross-env DEBUG=nightwatch-api node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand",
    "ci": "start-server-and-test http://localhost:3000",
    "lint": "eslint src",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "@babel/cli": "^7.0.0-beta.47",
    "@babel/core": "^7.0.0-beta.47",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.47",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.47",
    "@babel/preset-env": "^7.0.0-beta.47",
    "@babel/preset-typescript": "^7.0.0-beta.47",
    "debug": "^3.1.0",
    "nightwatch": "^1.0.6",
    "typescript": "^2.8.3"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "chromedriver": "2.38.3",
    "cross-env": "^5.1.5",
    "geckodriver": "1.11.0",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "lint-staged": "^7.1.0",
    "prettier": "1.12.1",
    "selenium-server": "3.11.0",
    "start-server-and-test": "^1.4.1",
    "wait-on": "^2.1.0"
  },
  "lint-staged": {
    "*.{js,json,css,md}": ["prettier --write", "git add"]
  }
}
